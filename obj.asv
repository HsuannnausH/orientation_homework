function f = obj(r1,r2,length)
%UNTITLED Summary of this function goes here
%   Detailed explanation goes here
    density = 7860;     % kg * m^-3
    f = 0;
    for i = 1:6
        f = f + density * pi * r1.^2 * length;
    end
    for i = 1:4
        f = f + density * pi * r2.^2 * length;
    end
end

